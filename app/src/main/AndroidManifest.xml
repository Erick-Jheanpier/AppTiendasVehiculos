<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.senati.apptiendasvehiculos">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppTiendasVehiculos"
        android:usesCleartextTraffic="true">


        <activity android:name=".Eliminar" android:exported="false" />
        <activity android:name=".Actualizar" android:exported="false" />
        <activity android:name=".Registrar" android:exported="false" />
        <activity android:name=".Listar" android:exported="false" />
        <activity android:name=".MainActivity" android:exported="true">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>
</manifest>
